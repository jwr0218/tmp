<Sysmon schemaversion="4.3">
    <!-- Process Creation -->
    <EventFiltering>
        <ProcessCreate onmatch="include"/>
    </EventFiltering>

    <!-- File Creation -->
    <EventFiltering>
        <FileCreate onmatch="include"/>
    </EventFiltering>

    <!-- Network Connection -->
    <EventFiltering>
        <NetworkConnect onmatch="include"/>
    </EventFiltering>

    <!-- Other Events -->
    <!-- 여기에 필요한 모든 이벤트 유형에 대한 설정을 추가합니다. -->

</Sysmon>
